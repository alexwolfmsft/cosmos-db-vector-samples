{
  "name": "ts-cosmos-nodejs-vector-samples",
  "version": "1.0.0",
  "description": "Samples for MongoDB vCore vector search with Cosmos DB",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start:one-insert": "node --env-file .env dist/insert-one-document.js",
